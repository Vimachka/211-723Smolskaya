№1  
#include <stdio.h>


int main() {
    float a, b, c;
    printf("Введите значение переменных А и В ");
    scanf("%f%f", &a, &b);
    c = a;
    a = b;
    b = c;
    printf("Новые значения: A = %.2f\n", a);
    printf ("B = %.2f\n", b);
    return 0;
    
}

№2 
#include <stdio.h>


int main() {
    float a, b, c, l, k;
    printf("Введите значение переменных А и В и С: ");
    scanf("%f%f%f", &a, &b, &c);
    k = a;
    a = b;
    l = b;
    b  = c;
    c = k;
    
    printf("Новые значения: A = %.2f\n", a);
    printf ("B = %.2f\n", b);
    printf("C = %.2f\n", c);
    return 0;
    
}

