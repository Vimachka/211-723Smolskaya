}


№4
#include <stdio.h>
#include <math.h>

int main() {
    float x, y;
    printf("Введите значение переменных x: ");
    scanf("%f", &x);
          y = 3 * pow(x, 6) - 6 * pow(x, 2) - 7;
    
          printf(" ответ = %.2f\n", y);
    return 0;
    
}

№5
#include <stdio.h>
#include <math.h>

int main() {
    float x, y;
    printf("Введите значение переменных x: ");
    scanf("%f", &x);
          y = 4 * pow((x-3), 6) - 7 * pow((x-3), 3) + 2;
    
          printf(" ответ = %.2f\n", y);
    return 0;
    
}

№6
#include <stdio.h>
#include <math.h>

int main() {
    float a, q;
    printf("Введите значение переменной a: ");
    scanf("%f", &a);
    q = a * a;
    
    q = q * q;
    
    
    
    printf("ответ = %.2f\n", q * q);
    return 0;
    
}
№ 7 (1)
#include <stdio.h>
#include <math.h>

int main() {
    float a, q, r;
    printf("Введите значение переменной a: ");
    scanf("%f", &a);
    q = a * a * a ;
    r = q * q ;
    
   
    
 
    
   

    printf("ответ = %.2f\n", r * r * q); 

    
}


(2)
#include <stdio.h>
#include <math.h>

int main() {
    float a, q, r;
    printf("Введите значение переменной a: ");
    scanf("%f", &a);
    q = a * a;
    q = q * q * q * q;
    r = q * q;
    
 
    
   
    
    
    
    printf("ответ = %.2f\n", r/a);  // в условии не было сказано про деление, но наверное, так неправильно по заданию.

    
}


