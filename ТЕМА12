#include <stdio.h>


int main (void) {
int x,a,b;
printf("Введите число в диапозоне 10-40: ");
scanf("%d", &x);

b = x%10;

a = x/10;
if (a==1)
{
switch (x) {
        case 10:
            printf ("Десять");
            break;
        case 11:
            printf("Одиннадцать");
            break;
        case 12:
            printf("Двендацать");
        break;
                case 13:
                    printf ("Тринадцать");
                    break;
                case 14:
                    printf("Четырнадцать");
                    break;
                case 15:
                    printf("Пятнадцать");
        break;
    case 16:
        printf ("Шестнадцать");
        break;
    case 17:
        printf("Семналцать");
        break;
    case 18:
        printf("Восемндацать");
        break;
    case 19:
        printf("Девятнадцать");
        break;
}
}
else if ((a!=1) && (x!=11) && (x!=11) && (x!=12) && (x!=13) && (x!=14) && (x!=15) && (x!=16) && (x!=17) && (x!=18) && (x!=19))  {
        switch (a) {
                case 2:
                    printf ("Двадцать ");
                    break;
            case 3:
                printf ("Тридцать ");
                break;
            case 4:
                printf ("Сорок ");
                break;
        }
        }
 if ((b>0) && (a!=1)){

                switch (b) {
                    case 1:
                        printf("одно");
                        break;
                    case 2:
                        printf ("два");
                        break;
                    case 3:
                        printf("три");
                        break;
                    case 4:
                        printf ("четыре");
                        break;
                    case 5:
                        printf("пять");
                        break;
                    case 6:
                        printf ("шесть");
                        break;
                    case 7:
                        printf("семь");
                        break;
                    case 8:
                        printf ("восемь");
                        break;
                    case 9:
                        printf("девять");
                        break;
                  
                }
}

printf(" учебных заданий\n");

return 0;
}

