#include <stdio.h>

int main (void) {
int n;
printf("Введите год, чтобы узнать его название:  ");
scanf("%d", &n);
    printf("год ");
    switch (n%10) {
          case 0:
            if (n%12==6 || n % 12 ==7 || n%12==8)
            printf("белого ");
            else
                printf("белой ");
            break;
        case 1:
            if (n%12==6 || n % 12 ==7 || n%12==8)
            printf("белого ");
            else
                printf("белой " );
            break;
        case 2:
            if (n%12==6 || n % 12 ==7 || n%12==8)
            printf("голубого ");
            else
                printf("голубой ");
            break;
        case 3:
            if (n%12==6 || n % 12 ==7 || n%12==8)
            printf("голубого ");
            else
                printf("голубой ");
            break;
        case 4:
            if (n%12==6 || n % 12 ==7 || n%12==8)
            printf("зелёного ");
            else
                printf("зелёной ");
            break;
        case 5:
            if (n%12==6 || n % 12 ==7 || n%12==8)
            printf("зелёного ");
            else
                printf("зелёной ");
            break;
        case 6:
            if (n%12==6 || n % 12 ==7 || n%12==8)
            printf("красного ");
            else
                printf("красной ");
            break;
        case 7:
            if (n%12==6 || n % 12 ==7 || n%12==8)
            printf("красного ");
            else
                printf("красной ");
            break;
        case 8:
            if (n%12==6 || n % 12 ==7 || n%12==8)
            printf("жёлтого ");
           else
               printf("жёлтой ");
            break;
        case 9:
            if (n%12==6 || n % 12 ==7 || n%12==8)
            printf("жёлтого");
            else
                printf("жёлтой");
            break;
            break;
    }
    
    switch (n%12) {
        case 0:
            printf("обезьяны");
            break;
        case 1:
            printf("курицы");
            break;
        case 2:
            printf("собаки");
            break;
        case 3:
            printf("свиньи");
            break;
        case 4:
            printf("крысы");
            break;
        case 5:
            printf("коровы");
            break;
        case 6:
            printf("тигра");
            break;
        case 7:
            printf("зайца");
            break;
        case 8:
            printf("дракона");
            break;
        case 9:
            printf("змеи");
            break;
        case 10:
            printf("лошади");
            break;
        case 11:
            printf("овцы");
            break;
    }
    
    printf("\n");

return 0;
    
}

